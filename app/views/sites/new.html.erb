<h1>New site</h1>

<% form_for(@site) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :slug, 'url' %><br />
    <%= f.text_field :slug %>.bunchup.us
  </p>

  <p>
    <%= f.label :feeds %><br />
    <%= f.text_area :feeds %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', sites_path %>
